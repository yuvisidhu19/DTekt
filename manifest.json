{
    "manifest_version": 3,
    "name": "DTekt",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "logo.png",
        "default_popup": "temp.html"
    },
    "permissions": ["tabs"]
}